<script type="text/javascript">
/* <![CDATA[ */
if (window.CSP_OK) {
      document.observe('dom:loaded', function(e) {
         blockHandler = false;
var localeToggle = function(e, localeKey) {
   if (Effect.Queues.get(localeKey).size() == 0) {
      $(localeKey + 'rolldown').toggle();
      $(localeKey + 'rollup').toggle();
      Effect.toggle(localeKey + 'details', 'blind', {
         duration: 0.6,
         queue: {
            position: 'end',
            scope: localeKey
         }
      });
   }
}
$('UStoggle').observe('click', localeToggle.bindAsEventListener(window, 'US'));
$('UKtoggle').observe('click', localeToggle.bindAsEventListener(window, 'UK'));
$('DEtoggle').observe('click', localeToggle.bindAsEventListener(window, 'DE'));
      });
}
/* ]]> */
</script>
