<script type="text/javascript">
/* <![CDATA[ */
if (window.CSP_OK) {
      document.observe('dom:loaded', function(e) {
         blockHandler = false;
	if (!window.universalKeepAlive) {
	      window.document.onclick = window.registerActivity;
	      window.document.ondblclick = window.registerActivity;
	      window.document.onmousemove = window.registerActivity;
		window.universalKeepAlive = new PeriodicalExecuter( function(pe) {
		   new Ajax.Request('/pub/utility/keepalive.do', {
		      evalJS :false,
		      evalJSON :false,
		      method :'post'
		   });
			   if (!window.userActivity) {
	            if (++window.inactiveCycles >= 3) {
                  universalAjaxHandler.handleItem(['passwordLoginRedirect', 'true']);
	            }
			   }
			   else {
			      window.userActivity = false;
			      window.inactiveCycles = 0;
			   }
		}, 540);
	}
      });
}
/* ]]> */
</script>
